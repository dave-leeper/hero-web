{
  "name": "hero-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.4",
    "chokidar-cli": "^2.1.0",
    "external": "file:./external",
    "hero_core-wasm": "^0.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.8.5",
    "react-awesome-router": "^2.0.0",
    "react-dom": "^16.8.5",
    "react-draggable": "^4.4.3",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "2.1.8",
    "tailwindcss": "^1.4.6",
    "react-app-rewired": "^2.1.5",
    "babel-plugin-import": "^1.12.2"
  },
  "scripts": {
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
    "start:react": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:tailwind": "tailwind build src/css/tailwind.css -o src/css/tailwind.output.css",
    "prebuild": "run-s build:tailwind",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/css/tailwind.output.css -c 'npm run build:tailwind'",
    "test": "react-app-rewired test",
    "svgr": "svgr -d src/components/ src/images/SVG"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@svgr/cli": "^5.4.0",
    "react-app-rewired": "^2.1.1",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "svg-inline-loader": "^0.8.2",
    "wasm-loader": "^1.3.0"
  }
}
